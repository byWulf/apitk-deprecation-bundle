{
  "name": "check24/apitk-deprecation-bundle",
  "version": "1.0.5",
  "license": "MIT",
  "type": "symfony-bundle",
  "description": "This bundle provides deprecation functionality for RESTful APIs.",
  "authors": [
    {
      "name": "Michael Wolf",
      "email": "wulf1337@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Shopping\\ApiTKDeprecationBundle\\": ""
    },
    "exclude-from-classmap": [
      "Tests/"
    ]
  },
  "repositories": [
    {
